<robot name="myRobot" 
    xmlns:xacro="http://www.ros.org/wiki/xacro">
    <!--Uthai Humanoid, FIBO, KMUTT, Thailand, 2017-->
    <!--This file generated by Liews Wuttipat-->
    <macro name="DH_fixed" params="parent child xyz rpy">
        <joint name="jd_${child}" type="fixed">
            <parent link="fd_${parent}" />
            <child link="f_${child}" />
            <origin rpy="${rpy}" xyz="${xyz}" />
        </joint>
        <link name="f_${child}" />
        <link name="fd_${child}" />
    </macro>
    <macro name="DH_F" params="parent child xyz rpy id">
        <DH_fixed child="${child}" parent="${parent}" rpy="${rpy}" xyz="${xyz}" />
        <joint name="jf_${id}" type="fixed">
            <parent link="f_${child}" />
            <child link="fd_${child}" />
        </joint>
    </macro>
    <macro name="DH_R" params="parent child xyz rpy id">
        <DH_fixed child="${child}" parent="${parent}" rpy="${rpy}" xyz="${xyz}" />
        <joint name="j_${id}" type="revolute">
            <parent link="f_${child}" />
            <child link="fd_${child}" />
            <axis xyz="0 0 1" />
            <limit effort="10" lower="-2.6179939" upper="2.6179939" velocity="5.6548668" />
        </joint>
    </macro>
    <!--End Macro to make dummy link-->
    <link name="base_link" />
